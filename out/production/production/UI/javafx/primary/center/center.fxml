<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="532.0" prefWidth="584.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.primary.center.CenterController">
    <center>
        <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" pannable="true" BorderPane.alignment="CENTER">
            <content>
                <VBox>
                    <children>
                        <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="20.0" VBox.vgrow="NEVER">
                            <children>
                                <Label text="Label" />
                                <ImageView pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER" />
                            </children>
                        </HBox>
                        <TableView fx:id="commitsTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn fx:id="messageTableColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="230.0" sortable="false" text="Message" />
                                <TableColumn fx:id="authorTableColumn" editable="false" minWidth="85.0" prefWidth="88.0" resizable="false" sortable="false" text="Author" />
                        <TableColumn fx:id="dateTableColumn" editable="false" minWidth="100.0" prefWidth="112.0" resizable="false" sortable="false" text="Date" />
                        <TableColumn fx:id="sha1TableColumn" editable="false" minWidth="100.0" prefWidth="100.0" resizable="false" sortable="false" text="SHA-1" />
                            </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                        </TableView>
                    </children>
                </VBox>
            </content>
        </ScrollPane>
    </center>
</BorderPane>
