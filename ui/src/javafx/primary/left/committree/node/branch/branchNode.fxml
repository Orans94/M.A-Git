<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="branchVBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="50.0" minHeight="-Infinity" minWidth="50.0" prefHeight="67.0" prefWidth="50.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.primary.left.committree.node.branch.BranchNodeController">
   <children>
      <Label fx:id="branchesNamesLabel" alignment="CENTER" onMouseClicked="#branchesNamesLabelMouseClicked">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <ImageView fx:id="arrowImageView" fitHeight="21.0" fitWidth="41.0" onMouseClicked="#arrowImageViewMouseClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@arrowWhite.png" />
         </image>
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </ImageView>
   </children>
</VBox>
