      </Label>
      <ChoiceBox fx:id="branchNamesChoiceBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="241.0">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
         </VBox.margin>
      </ChoiceBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Region prefHeight="74.0" prefWidth="304.0" />
            <Button fx:id="deleteBranchButton" mnemonicParsing="false" onAction="#deleteBranchAction" prefHeight="42.0" prefWidth="129.0" text="Delete branch">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
