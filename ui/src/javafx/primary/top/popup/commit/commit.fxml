<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="150.0" maxWidth="500.0" minHeight="150.0" minWidth="500.0" prefHeight="150.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.primary.top.popup.commit.CommitController">
   <children>
      <HBox maxHeight="75.0" maxWidth="500.0" minHeight="75.0" minWidth="500.0" prefHeight="75.0" prefWidth="500.0">
         <children>
            <Label prefHeight="29.0" prefWidth="98.0" text="Commit message: ">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Region prefHeight="75.0" prefWidth="172.0" />
            <TextField fx:id="commitMessageTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="149.0">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox maxHeight="75.0" maxWidth="500.0" minHeight="75.0" minWidth="500.0" prefHeight="75.0" prefWidth="500.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" prefHeight="38.0" prefWidth="80.0" text="Cancel">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Region prefHeight="75.0" prefWidth="272.0" />
            <Button fx:id="commitButton" mnemonicParsing="false" onAction="#commitAction" prefHeight="36.0" prefWidth="77.0" text="Commit">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
